.photo-list
  margin 0
  padding 0
  list-style none

  &__item
    display inline-block
    width 20.5%
    margin 0 6% 6% 0
    cursor pointer

    &:nth-child(4n)
      margin-right 0 

    img
      max-width 100%
      box-shadow 0px 0px 2px 3px rgba(0,0,0,.7)

  &__likes
    font-size 0.7em
    color #333
    float right

    &:hover
      text-decoration underline